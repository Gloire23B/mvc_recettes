{
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "^4.0",
        "cakephp/authentication": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Entity\\": "classes/",
            "App\\Controllers\\": "controllers/",
            "App\\Model\\": "model/"
        }
    },
    "config": {
        "vendor-dir": "vendor"
    },
    "extra": {
        "cakephp-expose-raw-queries": false
    }
}